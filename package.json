{
  "name": "react-h5",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "cmd": "esno ./build/commander/index.ts build",
    "cmd:r": "esno ./build/commander/index.ts config -r",
    "start": "esno ./build/commander/index.ts start",
    "build": "tsc && cross-env NODE_ENV=production vite build",
    "build:test": "tsc && cross-env vite build --mode test",
    "report": "cross-env REPORT=true npm run build",
    "preview": "vite preview",
    "eslint": "eslint src/**/*.{ts,tsx}",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "bootstrap": "pnpm install",
    "reinstall": "rm yarn.lock && rm package.lock.json && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install",
    "commit": "git-cz",
    "test": "test"
  },
  "dependencies": {
    "@unocss/reset": "^0.48.3",
    "ahooks": "^3.7.4",
    "antd-mobile": "^5.27.0",
    "axios": "^1.2.2",
    "lodash-es": "^4.17.21",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@iconify/json": "^2.2.3",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@unocss/preset-attributify": "^0.48.2",
    "@unocss/preset-icons": "^0.48.2",
    "@unocss/preset-uno": "^0.48.2",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-react": "^3.0.0",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.13",
    "chalk": "^5.2.0",
    "commander": "^9.5.0",
    "conventional-changelog-gitmoji-config": "^1.4.4",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "esno": "^0.16.3",
    "figlet": "^1.5.2",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "lint-staged": "12.5.0",
    "os": "^0.1.2",
    "postcss-html": "^1.5.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.0",
    "typescript": "^4.9.3",
    "unocss": "^0.48.2",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-purge-icons": "^0.9.2",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-top-level-await": "^1.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
